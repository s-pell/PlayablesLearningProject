:root {
}

.unity-progress-bar {
    border-top-left-radius: 10px;
    align-self: stretch;
    align-content: center;
    justify-content: space-around;
    border-top-right-radius: 10px;
    border-bottom-left-radius: 10px;
    border-bottom-right-radius: 10px;
    background-color: rgb(59, 242, 173);
    border-left-color: rgba(209, 223, 90, 0);
    border-right-color: rgba(209, 223, 90, 0);
    border-top-color: rgba(209, 223, 90, 0);
    border-bottom-color: rgba(209, 223, 90, 0);
}
